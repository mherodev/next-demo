{
  "name": "next-demo",
  "version": "1.0.0",
  "description": "A Next.js demo site.",
  "repository": "TBD",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm run nodecheck",
    "postinstall": "[ -f .envrc ] || npm run envrc:reset",
    "analyze": "ANALYZE=true npm run build:next",
    "build": "run-s build:clean build:server build:next build:copy",
    "build:clean": "rm -rf ./build ./src/.next && npm run clean:babel-loader",
    "build:next": "NODE_ENV=production npx next build src",
    "build:server": "NODE_ENV=production npx babel src --out-dir build",
    "build:copy": "cp -r src/.next/ build/.next/ && cp -r src/static build/",
    "build:lib": "rm -rf ./local_modules/*/dist && npm install",
    "clean": "npx run-s clean:files clean:babel-loader clean:jest",
    "clean:files": "rm -rf ./build ./local_modules/*/dist ./node_modules ./src/.next",
    "clean:babel-loader": "rm -rf node_modules/.cache/babel-loader",
    "clean:jest": "npx jest --clearCache",
    "clean:install": "npm run clean && npm install",
    "dev": "NODE_ENV=development nodemon",
    "dev:nodemon-exec": "npm run dev:babel-node",
    "dev:babel-node": "babel-node ./src/server.js",
    "envrc:reset": "cp -f .envrc .envrc_old 2> /dev/null ; cp -f .envrc_sample .envrc && direnv allow",
    "lint": "npx eslint src",
    "nodecheck": "node ./run_scripts/nodeVersion.js",
    "start": "NODE_ENV=production node build/server.js",
    "stop-all": "killall node",
    "test": "BABEL_ENV=test npx jest --config ./test/jest.config.js",
    "test:coverage": "npm test -- --coverage --changedSince=master",
    "test:coverage:all": "npm test -- --coverage",
    "test:coverage:open": "npm run test:coverage ; open -a \"Google Chrome\" ./test/coverage/index.html",
    "test:update": "npm test -- --updateSnapshot",
    "test:watch": "npm test -- --watch --updateSnapshot"
  },
  "author": "Michael Herold <mherodev@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@next/bundle-analyzer": "^8.1.0",
    "@yolkai/next-routes": "^2.0.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cookies": "^0.7.3",
    "dd-trace": "0.11.2",
    "debug": "^4.1.1",
    "emotion": "^10.0.6",
    "emotion-server": "^10.0.6",
    "emotion-theming": "^10.0.6",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "http-status-codes": "^1.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.14",
    "next": "^9.1.1",
    "next-compose-plugins": "^2.2.0",
    "next-progressbar": "^1.0.0",
    "next-transpile-modules": "^2.3.1",
    "path-to-regexp": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "ajv": "^6.7.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-emotion": "^10.0.13",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-define": "^1.3.1",
    "chalk": "^2.4.2",
    "css-loader": "^2.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-emotion": "^10.0.6",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "git-repo-info": "^2.1.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.3.0",
    "lolex": "^4.1.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.7.0",
    "to-string-loader": "^1.1.5"
  }
}
